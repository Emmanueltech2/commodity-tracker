      <bind nodeset="/cholera_suspicion_follow_up/data/meta/__delivery_uuid" type="string" calculate="../../../inputs/delivery_uuid"/>
      <bind nodeset="/cholera_suspicion_follow_up/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
