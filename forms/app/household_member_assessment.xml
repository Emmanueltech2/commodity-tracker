      <bind nodeset="/household_member_assessment/group_review/facility_recommendation" readonly="true()" type="string" relevant="selected( /household_member_assessment/household_member_assessment/patient_well_being , 'worsening') or selected( /household_member_assessment/household_member_assessment/patient_well_being , 'emergency')"/>
      <bind nodeset="/household_member_assessment/group_review/findings_assessment" readonly="true()" type="string"/>
