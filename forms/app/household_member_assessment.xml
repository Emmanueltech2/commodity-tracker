      <bind nodeset="/household_member_assessment/group_review/summary_name" readonly="true()" type="string"/>
      <bind nodeset="/household_member_assessment/group_review/complete_action" readonly="true()" type="string"/>
      <bind nodeset="/household_member_assessment/group_review/all_patient_details" readonly="true()" type="string"/>
