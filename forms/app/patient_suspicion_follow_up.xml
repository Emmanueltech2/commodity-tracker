      <bind nodeset="/patient_suspicion_follow_up/data/__vomits_everything" type="string" calculate=" /patient_suspicion_follow_up/danger_signs/vomit "/>
      <bind nodeset="/patient_suspicion_follow_up/data/__drowsy_or_unconscious" type="string" calculate=" /patient_suspicion_follow_up/danger_signs/drowsy "/>
