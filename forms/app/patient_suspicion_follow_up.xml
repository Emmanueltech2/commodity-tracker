      <bind nodeset="/patient_suspicion_follow_up/data/__fever" type="string" calculate=" /patient_suspicion_follow_up/danger_signs/fever "/>
      <bind nodeset="/patient_suspicion_follow_up/data/__blue_skin_color" type="string" calculate=" /patient_suspicion_follow_up/danger_signs/blue_skin "/>
      <bind nodeset="/patient_suspicion_follow_up/data/__has_danger_sign" type="string" calculate=" /patient_suspicion_follow_up/danger_signs/r_danger_sign_present "/>
      <bind nodeset="/patient_suspicion_follow_up/data/meta/__patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/patient_suspicion_follow_up/data/meta/__patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/patient_suspicion_follow_up/data/meta/__household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/patient_suspicion_follow_up/data/meta/__source" type="string" calculate="../../../inputs/source"/>
