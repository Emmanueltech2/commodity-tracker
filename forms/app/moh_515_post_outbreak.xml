<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>MOH 515 (Post Outbreak)</h:title>
    <model>
      <instance>
        <moh_515_post_outbreak id="moh_515_post_outbreak" prefix="J1!moh_515_post_outbreak!" delimiter="#" version="`1.0`">
          <form_summary>
            <cha_name/>
            <cha_area/>
            <cha_facility/>
            <cha_county/>
            <chp_total/>
            <chp_reported_total/>
          </form_summary>
          <household_indicators>
            <total_households/>
            <new_households/>
            <new_households_visited/>
            <new_households_with_clean_water/>
            <new_households_with_latrines/>
          </household_indicators>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </moh_515_post_outbreak>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/moh_515_post_outbreak/form_summary/cha_name" type="db:person" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/form_summary/cha_area" type="string" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/form_summary/cha_facility" type="string" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/form_summary/cha_county" type="string" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/form_summary/chp_total" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/form_summary/chp_reported_total" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/household_indicators/total_households" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/household_indicators/new_households" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/household_indicators/new_households_visited" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/household_indicators/new_households_with_clean_water" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/household_indicators/new_households_with_latrines" type="int" required="true()"/>
      <bind nodeset="/moh_515_post_outbreak/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/moh_515_post_outbreak/form_summary">
      <label>Form Summary</label>
      <input ref="/moh_515_post_outbreak/form_summary/cha_name">
        <label>What Is Your Name?</label>
      </input>
      <input ref="/moh_515_post_outbreak/form_summary/cha_area">
        <label>What Is Your Area?</label>
      </input>
      <input ref="/moh_515_post_outbreak/form_summary/cha_facility">
        <label>What Is Your Linked Facility?</label>
      </input>
      <input ref="/moh_515_post_outbreak/form_summary/cha_county">
        <label>What County Do You Belong To?</label>
      </input>
      <input ref="/moh_515_post_outbreak/form_summary/chp_total">
        <label>How Many CHPs Are In Your Area?</label>
      </input>
      <input ref="/moh_515_post_outbreak/form_summary/chp_reported_total">
        <label>How Many CHPs Submitted Monthly Report?</label>
      </input>
    </group>
    <group ref="/moh_515_post_outbreak/household_indicators">
      <label>Household Indicators</label>
      <input ref="/moh_515_post_outbreak/household_indicators/total_households">
        <label>Total Households In The Area?</label>
      </input>
      <input ref="/moh_515_post_outbreak/household_indicators/new_households">
        <label>Number Of New Households Registered This Month?</label>
      </input>
      <input ref="/moh_515_post_outbreak/household_indicators/new_households_visited">
        <label>Number Of New Households Visited This Month?</label>
      </input>
      <input ref="/moh_515_post_outbreak/household_indicators/new_households_with_clean_water">
        <label>Number Of New Households Visited This Month With Clean Water Access?</label>
      </input>
      <input ref="/moh_515_post_outbreak/household_indicators/new_households_with_latrines">
        <label>Number Of New Households Visited This Month With Latrines/Toilets?</label>
      </input>
    </group>
  </h:body>
</h:html>
